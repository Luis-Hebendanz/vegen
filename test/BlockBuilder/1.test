RUN: %test-block-builder -test1 | FileCheck %s

CHECK: entry:
CHECK-NEXT:   br i1 %x1, label %0, label %1

CHECK: 0:
CHECK-NEXT:   br label %3

CHECK: 1:
CHECK-NEXT:   br label %8

CHECK: 2:
CHECK-NEXT:   %DUMMY
CHECK-NEXT:   br label %8

CHECK: 3:
CHECK-NEXT:   %4 = and i1 true, %x2
CHECK-NEXT:   %5 = and i1 true, %x3
CHECK-NEXT:   %6 = or i1 %4, %5
CHECK-NEXT:   br i1 %6, label %2, label %7

CHECK: 7:
CHECK-NEXT:   br label %8

CHECK: 8:
CHECK-NEXT:   ret void
