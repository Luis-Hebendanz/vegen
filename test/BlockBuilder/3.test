RUN: %test-block-builder -test3 | FileCheck %s

CHECK: entry:
CHECK-NEXT:   br i1 %x1, label %0, label %1

CHECK: 0:
CHECK-NEXT:   %DUMMY-if-x1
CHECK-NEXT:   br label %2

CHECK: 1:
CHECK-NEXT:   br label %3

CHECK: 2:
CHECK-NEXT:   %DUMMY-x1-or-x2
CHECK-NEXT:   br label %5

CHECK: 3:
CHECK-NEXT:   br i1 %x2, label %2, label %4

CHECK: 4:
CHECK-NEXT:   br label %5

CHECK: 5:
CHECK-NEXT:   ret void
