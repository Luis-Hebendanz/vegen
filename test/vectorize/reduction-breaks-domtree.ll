  %cmp2 = fcmp ugt double %add.lcssa, 1.000000e+01
  br i1 %cmp2, label %if.then, label %if.end

if.then:                                          ; preds = %for.end
  call void @abort() #4
  unreachable

if.end:                                           ; preds = %for.end
  call void @free(i8* %call)
  ret void
}

